  <DataSets>
    <DataSet Name="GLAdjustmentData">
      <Query>
        <DataSourceName>GLAdjustmentDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MyReconciliationCategoryTypeID">
            <Value>=Parameters!MyReconciliationCategoryTypeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GLdataID">
            <Value>=Parameters!GLdataID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Status">
            <Value>=Parameters!Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select * from(
SELECT 
		GRI.[GLDataRecItemID]
		, GRI.[GLDataID]
		, GRI.[ReconciliationCategoryID]
		, X.[ReconciliationCategoryTypeID]
		, GRI.[DataImportID]
		, GRI.[IsAttachmentAvailable]
		,GRI.[Amount]  As Amount
		, GRI.[LocalCurrencyCode]
		,GRI.[AmountBaseCurrency] As AmountBaseCurrency
		, GRI.[AmountReportingCurrency] As AmountReportingCurrency
		,GRI.[OpenDate]  As OpenDate
		, GRI.[CloseDate] As CloseDate
		, GRI.[JournalEntryRef]
		, GRI.[Comments]
		, GRI.[CloseComments]
		,GRI.[DateAdded] As DateAdded
		, GRI.[AddedBy]
		, GRI.[RecItemNumber] 
		, GRI.[PreviousGLDataRecItemID]
		, GRI.[OriginalGLDataRecItemID]
		, AttachmentCount = ISNULL((SELECT SUM(1) FROM [dbo].[Attachment] A 
									WHERE GRI.GLDataRecItemID = A.RecordID 
									AND A.RecordTypeID = 2 
									AND A.IsActive = 1),0)
		, IsForwardedItem = (CASE 
							   WHEN GRI.PreviousGLDataRecItemID IS NULL
							   THEN CAST (0 AS bit)
							   ELSE CAST (1 AS bit) 
							   END)
		,
		CASE WHEN CloseDate IS NULL Then 'Open'
			 Else 'Closed' 
			 END as 'Status',
		CASE WHEN GRI.ReconciliationCategoryTypeID IN (26,27,28) Then 'GLAdjustment'
				  ELSE 'Timing Difference' END AS 'Sections',
	   MSH.MatchSetRef As MatchSetRef
		 
		               
	FROM 
	ReconciliationCategoryTypeMst  X
	Left outer join
	(Select * from GLDataRecItem  Where GLdataID =@GLdataID
And IsActive=1) GRI
	on GRI.ReconciliationCategoryTypeID = X.ReconciliationCategoryTypeID 
	LEFT OUTER JOIN Matching.MatchSetSubSetCombination MSC
			 ON (GRI.RecordSourceID=MSC.MatchSetSubSetCombinationID 
			     AND ISNULL(GRI.RecordSourceTypeID,0)=2)   
		LEFT OUTER JOIN Matching.MatchSetMatchingSourceDataImport  MSM
			 ON MSC.MatchSetMatchingSourceDataImport1ID=MSM.MatchSetMatchingSourceDataImportID
		LEFT OUTER JOIN  Matching.MatchSetHdr MSH
			 ON MSM.MatchSetID=MSH.MatchSetID
	Where
	X.ReconciliationCategoryTypeID =  @MyReconciliationCategoryTypeID
	) as T
	where T.[Status]=@Status AND T.[GLDataID]=@GLdataID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
