  <DataSets>
    <DataSet Name="RecCategoryData">
      <Query>
        <DataSourceName>RecCategorySummaryDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MyReconciliationCategoryTypeID">
            <Value>=Parameters!MyReconciliationCategoryTypeID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@GLdataID">
            <Value>=Parameters!GLdataID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReconciliationTemplateID">
            <Value>=Parameters!ReconciliationTemplateID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
		  @MyReconciliationCategoryTypeID As ReconciliationCategoryTypeIDTotal
		, sum(GRI.[Amount]) As AmountTotal
		,sum(GRI.[AmountBaseCurrency]) As AmountBaseCurrencyTotal
		, sum(GRI.[AmountReportingCurrency])  As AmountReportingCurrencyTotal
	 
		               
	FROM 
	ReconciliationCategoryTypeMst  X
	Left outer join
	(Select * from GLDataRecItem  Where GLdataID =   @GLdataID and CloseDate is null and IsActive=1) GRI
	on GRI.ReconciliationCategoryTypeID =  @MyReconciliationCategoryTypeID
	Where
	X.ReconciliationCategoryTypeID IN (select ReconciliationCategoryTypeID from ReconciliationCategoryTypeMst
	where (ReconciliationTemplateID =@ReconciliationTemplateID AND ReconciliationCategoryTypeID &lt;&gt; 39 AND X.ReconciliationCategoryTypeID = @MyReconciliationCategoryTypeID))
	group by X.[ReconciliationCategoryTypeID]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ReconciliationCategoryTypeIDTotal">
          <DataField>ReconciliationCategoryTypeIDTotal</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="AmountTotal">
          <DataField>AmountTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AmountBaseCurrencyTotal">
          <DataField>AmountBaseCurrencyTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="AmountReportingCurrencyTotal">
          <DataField>AmountReportingCurrencyTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FormatData">
      <Query>
